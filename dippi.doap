  xmlns:gnome="http://api.gnome.org/doap-extensions#"
  xmlns="http://usefulinc.com/ns/doap#"
>
  <name>Dippi</name>
  <shortdesc xml:lang="en">Calculate display info like DPI</shortdesc>
  <description xml:lang="en">Analyze any display. Input a few simple details and figure out the aspect ratio, DPI, and other details of a particular display. Great for deciding which laptop or external monitor to purchase, and if it would be considered HiDPI.</description>
  <category rdf:resource="http://api.gnome.org/doap-extensions#apps" />

  <homepage rdf:resource="https://cassidyjames.com/dippi" />
  <download-page rdf:resource="https://flathub.org/apps/com.github.cassidyjames.dippi" />
  <bug-database rdf:resource="https://github.com/cassidyjames/dippi/issues" />

  <programming-language>Vala</programming-language>

